    <Method Name="Disable" Id="{b2c22a57-475f-4789-b3a0-b0e9911e4299}">
      <Declaration><![CDATA[METHOD Disable : bool
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF (_Axis = 0) THEN
	RETURN;
END_IF

_Busy		 := TRUE;
_Error		 := FALSE;
_ErrorID	 := 0;
SequenceStep := E_ExternalSPGen.DisableCmd;
Disable		 := TRUE;]]></ST>
