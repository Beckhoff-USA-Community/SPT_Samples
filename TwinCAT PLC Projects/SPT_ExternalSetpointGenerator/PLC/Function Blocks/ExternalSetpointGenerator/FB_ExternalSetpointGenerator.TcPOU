	_Position	 : LREAL;
	_Destination : LREAL := 0.0;
	_Velocity	 : LREAL := 0.0;
	_Accel		 : LREAL := 0.0;
	_Direction	 : DINT	 := 1;

	// FBs
	EnableExtSP	 : MC_ExtSetPointGenEnable;
	DisableExtSP : MC_ExtSetPointGenDisable;

	//Internal Vars
	SequenceStep : INT;
	Options		 : ST_ExtSetPointEnableOptions;
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[]]></ST>
